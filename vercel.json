{
  "crons": [
    {
      "path": "/api/cron/sync-views",
      "schedule": "0 6 * * *"
    },
    {
      "path": "/api/cron/flush-watchtime",
      "schedule": "0 7 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/npm/@clerk/:path*",
      "destination": "https://cdn.clerk.io/npm/@clerk/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(js|css|woff|woff2|ttf|otf|eot)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)\\.(jpg|jpeg|png|gif|webp|avif|svg|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
